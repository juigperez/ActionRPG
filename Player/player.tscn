script/source = "extends CharacterBody2D

@export var ACCELERATION = 500            # Ajustar para mejorar playfeel
@export var MAX_SPEED = 80                # Ajustar para mejorar playfeel
@export var FRICTION = 500                # Ajustar para mejorar playfeel
@export var ROLL_SPEED_MULTIPLIER = 1.5   # Ajustar para mejorar playfeel
var ROLL_SPEED = MAX_SPEED * ROLL_SPEED_MULTIPLIER

enum{
	MOVE,
	ROLL,          #Enumerador --> MOVE es 0, ROLL es 1, ATTACK es 2
	ATTACK
}

var state = MOVE
var roll_vector = Vector2.DOWN

@onready var animationPlayer = $AnimationPlayer
@onready var animationTree = $AnimationTree
@onready var animationState = animationTree.get(\"parameters/playback\")
@onready var swordHitbox = $HitboxPivot/SwordHitbox

func _ready():
	animationTree.active = true
	swordHitbox.knockback_vector = roll_vector
	
func _physics_process(delta):
	match state:                          # esto es lo mismo que hacer if state == MOVE:
		MOVE:                             #                                move_state(delta)
			move_state(delta)             #                            elif state == ROLL: (etc)
		ROLL:
			roll_state()
		ATTACK:
			attack_state()

func move_state(delta):
	var input_vector = Vector2.ZERO
	input_vector.x = Input.get_action_strength(\"ui_right\") - Input.get_action_strength(\"ui_left\")
	input_vector.y = Input.get_action_strength(\"ui_down\") - Input.get_action_strength(\"ui_up\")
	input_vector = input_vector.normalized()
	
	if input_vector != Vector2.ZERO:
		roll_vector = input_vector
		swordHitbox.knockback_vector = input_vector
		animationTree.set(\"parameters/Idle/blend_position\", input_vector)
		animationTree.set(\"parameters/Run/blend_position\", input_vector)
		animationTree.set(\"parameters/Attack/blend_position\", input_vector) # Lo seteo ac치 porque no quiero tener que pasarle input_vector a attack_state() y porque quiero que no pueda cambiar de direcci칩n durante el ataque
		animationTree.set(\"parameters/Roll/blend_position\", input_vector)   # Lo seteo ac치 porque no quiero tener que pasarle input_vector a roll_state() y porque quiero que no pueda cambiar de direcci칩n durante el ataque
		animationState.travel(\"Run\")
		velocity = velocity.move_toward(input_vector * MAX_SPEED, ACCELERATION * delta)
	else:
		animationState.travel(\"Idle\")
		velocity = velocity.move_toward(Vector2.ZERO, FRICTION * delta)
	
	move_and_slide()
	
	if Input.is_action_just_pressed(\"roll\"):
		state = ROLL
	elif Input.is_action_just_pressed(\"attack\"):
		state = ATTACK

	
func roll_state():
	velocity = roll_vector * ROLL_SPEED
	animationState.travel(\"Roll\")
	move_and_slide()
	
func attack_state():
	velocity = Vector2.ZERO
	animationState.travel(\"Attack\")
	

func attack_animation_finished():
	state = MOVE

func roll_animation_finished():
	velocity = velocity / 2  #Para que no deslice tanto al final de la animacion
	state = MOVE
"

